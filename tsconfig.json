{
  "compilerOptions": {
    "target": "esnext",
    "module": "esnext",
    "moduleResolution": "node",
    "importHelpers": true,
    "isolatedModules": true,
    "noEmit": true,
    "allowJs": true,
    "paths": {
      "@/*": [
        "./src/*"
      ],
      "@components/*": [
        "./src/components/*"
      ],
      "@sections/*": [
        "./src/sections/*"
      ],
      "@utils/*": [
        "./src/utils/*"
      ],
    }
  },
  "vueCompilerOptions": {
    "experimentalDisableTemplateSupport": true
  }
}